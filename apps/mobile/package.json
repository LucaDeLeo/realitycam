{
  "name": "@realitycam/mobile",
  "version": "0.1.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios",
    "prebuild": "expo prebuild --platform ios",
    "lint": "eslint .",
    "test": "vitest run",
